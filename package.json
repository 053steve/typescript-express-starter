{
  "name": "angular-typescript-starter",
  "version": "1.0.0",
  "description": "BE",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-live": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes --ignore '*/routes/*.ts'\"",
    "dev": "nodemon",
    "start": "npm run preprocess && ts-node ./dist/lib/local.js",
    "offline": "sls offline start --useSeparateProcesses",
    "tsoa:gen": "node node_modules/tsoa/dist/cli.js spec-and-routes",
    "preprocess": "npm run tsoa:gen",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "package": "yarn build && cp -rf lib/swagger dist/lib && serverless package",
    "deploy": "serverless deploy -v --stage production"
  },
  "dependencies": {
    "@metamask/eth-sig-util": "^4.0.0",
    "@mikro-orm/mysql": "^4.0.7",
    "@types/aws-serverless-express": "^3.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.11.1",
    "@types/validator": "^13.1.0",
    "aws-sdk": "^2.582.0",
    "aws-serverless-express": "^3.1.0",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "dot-object": "^2.1.4",
    "ethereumjs-util": "^7.1.3",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "moment-timezone": "^0.5.31",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "5.22.3",
    "serverless-http": "^2.0.2",
    "shortid": "^2.2.15",
    "source-map-support": "^0.5.10",
    "swagger-ui-express": "^4.1.4",
    "tsoa": "^3.2.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/node": "^14.11.8",
    "concurrently": "^5.3.0",
    "csv-parser": "^2.3.3",
    "ejs": "^3.1.5",
    "fs-extra": "^9.0.1",
    "nodemon": "^1.19.1",
    "serverless-offline": "^5.3.3",
    "serverless-webpack": "^5.2.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2",
    "yargs-parser": "^20.2.4"
  },
  "author": "053steve",
  "license": "MIT"
}
